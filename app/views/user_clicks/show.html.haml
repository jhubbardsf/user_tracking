- model_class = UserClick
.page-header
  %h1=t '.title', :default => model_class.model_name.human.titleize

%p
  %strong= model_class.human_attribute_name(:user_id) + ':'
  %br
  = @user_click.user_id
%p
  %strong= model_class.human_attribute_name(:field) + ':'
  %br
  = @user_click.field


= link_to t('.back', :default => t("helpers.links.back")), user_clicks_path, :class => 'btn btn-default'
= link_to t('.edit', :default => t("helpers.links.edit")), edit_user_click_path(@user_click), :class => 'btn btn-default'
= link_to t('.destroy', :default => t("helpers.links.destroy")), user_click_path(@user_click), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
